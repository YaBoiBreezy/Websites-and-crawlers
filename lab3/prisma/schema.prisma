generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("SQLITE_DB_URL")
}

model Page {
  id        Int      @id @default(autoincrement())
  url       String   @unique
  content   String   @map("content")
  visitedAt DateTime @default(now())
  outgoing  Page[]   @relation("incoming")
  incoming  Page[]   @relation("incoming")
}
